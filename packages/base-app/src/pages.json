{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uv-(.*)": "@climblee/uv-ui/components/uv-$1/uv-$1.vue"
    }
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "condition": {
    "current": 0,
    "list": []
  },
  "pages": [
    {
      "path": "packages/main/pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    }
  ],
  "subPackages": [
    {
      "root": "packages/sundry",
      "name": "sundryPack",
      "pages": [
        {
          "path": "pages/index/index",
          "style": {
            "navigationBarTitleText": "sundryPack"
          }
        }
      ]
    }
  ]
}
